    <ControlTemplate x:Key="HorizontalScrollBar" TargetType="{x:Type ScrollBar}">
        <Grid Height="20">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition MaxWidth="10"/>
                    <ColumnDefinition Width="0.00001*"/>
                    <ColumnDefinition MaxWidth="10"/>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0">
                </Grid>
                <Grid Grid.Column="1">
                    <Rectangle Fill="#22FFFFFF" />
                    <Path Stretch="Fill" Stroke="#FF9A9A9A" StrokeEndLineCap="Square" StrokeStartLineCap="Square" Margin="0" VerticalAlignment="Top" Height="1" Data="M0.5,0.5 L79.5,0.5"/>
                    <Path Stretch="Fill" Stroke="#FF9A9A9A" StrokeEndLineCap="Square" StrokeStartLineCap="Square" Margin="0" VerticalAlignment="Bottom" Height="1" Data="M79.5,0.5 L0.5,0.5"/>
                </Grid>
                <Grid Grid.Column="2">
                </Grid>
            </Grid>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition MaxWidth="43"/>
                    <ColumnDefinition Width="0.00001*" />
                    <ColumnDefinition MaxWidth="43"/>
                </Grid.ColumnDefinitions>

                <RepeatButton Grid.Column="0"
                    Style="{StaticResource ScrollBarButtonLeft}"
                    Width="43"
					Command="ScrollBar.LineLeftCommand"
                    Content="M 4 0 L 4 8 L 0 4 Z" />

                <Track x:Name="PART_Track"
             Grid.Column="1"
			 ViewportSize="NaN"
             IsDirectionReversed="False">
                    <Track.DecreaseRepeatButton>
                        <RepeatButton Style="{StaticResource ScrollBarPageButton}"
                        Command="ScrollBar.PageLeftCommand" />
                    </Track.DecreaseRepeatButton>
                    <Track.IncreaseRepeatButton>
                        <RepeatButton Style="{StaticResource ScrollBarPageButton}"
                        Command="ScrollBar.PageRightCommand" />
                    </Track.IncreaseRepeatButton>
                    <Track.Thumb>
                        <Thumb Style="{StaticResource ScrollBarThumb}" Width="50"/>
                    </Track.Thumb>
                </Track>

                <RepeatButton Grid.Column="3"
                    Style="{StaticResource ScrollBarButtonRight}"
                    Width="43"
                    Command="ScrollBar.LineRightCommand"
                    Content="M 0 0 L 4 4 L 0 8 Z" />
            </Grid>
        </Grid>
    </ControlTemplate>

    <Style x:Key="{x:Type ScrollBar}"
         TargetType="{x:Type ScrollBar}">
        <Setter Property="SnapsToDevicePixels"
            Value="True" />
        <Setter Property="OverridesDefaultStyle"
            Value="true" />
        <Style.Triggers>
            <Trigger Property="Orientation"
               Value="Horizontal">
                <Setter Property="Width"
                Value="Auto" />
                <Setter Property="Height"
                Value="18" />
                <Setter Property="Template"
                Value="{StaticResource HorizontalScrollBar}" />
            </Trigger>
            <Trigger Property="Orientation"
               Value="Vertical">
                <Setter Property="Width"
                Value="18" />
                <Setter Property="Height"
                Value="Auto" />
                <!--<Setter Property="Template"
                Value="{StaticResource VerticalScrollBar}" />-->
            </Trigger>
        </Style.Triggers>
    </Style>
